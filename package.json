{
  "name": "xwing-cardgen-desktop",
  "version": "0.1.0",
  "description": "X-Wing Pilot Card Generator",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "node_modules/.bin/electron-packager ./app/ --all --asar --out ./dist/ --overwrite",
    "package-linux": "node_modules/.bin/electron-packager ./app/ --platform=linux --arch=x64 --asar --out ./dist/ --overwrite",
    "package-mac": "node_modules/.bin/electron-packager ./app/ --platform=darwin --arch=x64 --asar --out ./dist/ --overwrite"
  },
  "keywords": [
    "xwing",
    "artwork",
    "generator",
    "desktop"
  ],
  "devDependencies": {
    "electron": "^1.4.10",
    "electron-packager": "^8.5.1",
    "less-watch-compile": "^1.1.8"
  },
